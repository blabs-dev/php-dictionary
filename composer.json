{
    "name": "blabs-dev/php-dictionary",
    "description": "A simple abstract class that let you write quick dictionaries using constants",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Salvo Bonanno",
            "email": "salvob.labs@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.1|8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Blabs\\Dictionary\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blabs\\Dictionary\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
